function _update60()
 ot=tt
 tt=time()--*(0.05+sn_hit*0.028)
 
 snc=cocreate(sync)
	
 if snc and costatus(snc)!= 'dead' then
  coresume(snc)
 else
  snc=nil
 end 
 
 update_poly(hitf)
 
 dt=time()-tt
end


function sync()
 cot=cos(f*0.004)
 sit=sin(f*0.004)
 xoff=sin(tt*0.02)*20
 yoff=sin(tt*0.03)*22
 b=15--rnd_efu-btn()
 bb=stat(26)/3.
 pattern=stat(24)
