function _init()
 --[
 -- palette gradient
 poke4(0x5f10,0x8582.8000)
 poke4(0x5f14,0x0706.8605)
 poke4(0x5f18,0x088e.0987)
 poke4(0x5f1c,0x8084.0288)
 poke(0x5f2e,1)
 --]]
	-- music init

	poke(0x5f40,0b1111)
	poke(0x5f41,0b0000)
	poke(0x5f42,0b0001)
	poke(0x5f43,0b0001)
	
	music(0)

end 

function _update60()
 tt=time()*(0.05+bd_hit*0.028)
 cot=cos(tt+t*2)
 sit=sin(tt-t*2)
 xoff=sin(tt*0.02)*20
 yoff=sin(tt*0.03)*22
 b=15-btn()
 bb=stat(26)/3.
 pattern=stat(24)
 if flr(bb%16)==8 or flr(bb%16)==9 then
  sn_hit=2
 else
 	sn_hit=0
 end
 if pattern%8==0 and (flr(bb)==0 or flr(bb)==1) then
  bd_hit=1
 elseif pattern%8==1 and (flr(bb)==0 or flr(bb)==1) then
  bd_hit=0
 else
  bd_hit=bd_hit*0.95
 end
 
 t=t*0.9+sn_hit*0.001
 if rec==1 then
  update_music_stats()
 end
end



function _draw()
--[
 memcpy(0x1000,0x6000,0x2000)
 rectfill(0,0,128,128,15)
 	for mem=scrstart,scrend do
 	if flr(mem>>>6)%2==0 then
 	 mem=mem-(scrstart)
 	 x=((mem%64-32)*cot-((mem>>>7)-32)*sit)
 	 y=((mem%64-32)*sit+((mem>>>7)-32)*cot)
 		mem=mem+(scrstart)
 		c=(x^^y)*0.2+t*300--(1+sin(t)*8)
 		p=@(mem-0x5000-bd_hit)
 		p1=(flr(bd_hit*4)*(p+1))|((c&b))
 		p2=(flr(bd_hit*4)*(p+1))|((c&b))
  	poke(mem,((p2<<4)|p1)) 
  	poke(mem-64,((p2<<4)|p1))
 	end
 	end
 if rec==1 then
 rectfill(0,0,31,128,0)
  draw_music_stats()
 end
 rec=btn()>>>12&1
 --]]
 --[[
 rectfill(0,0,128,128,0)
 for i=0,15 do
  print(@(0x5f10+i),0,6*i,i)
  
  print(i,4*4,6*i,i)
 end
 --]]
end




function stats(i,k,x,y,text)
 local note={}
	print("",x,y,7)
	print(text)
	for li=i,k do
	 color(8)
	 print(stat(li))
	 note=get_note(li-16)
	 rectfill(x+8,y+6*(li-15),x+8+note[3]*2+1,y+6*(li-15)+4,note[3])
	end
end

function update_music_stats()

	ch1=get_note(0)
	ch2=get_note(1)
	ch3=get_note(2)
	ch4=get_note(3)
end

