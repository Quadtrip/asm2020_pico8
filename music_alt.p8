 sefx={ch1[3],ch2[3],ch3[3],ch4[3]}
 volu={ch1[4],ch2[4],ch3[4],ch4[4]}
 if note[2]==36 and inst[2]==6 and ti[2]+0.1>tt then
  hitf[1]=5
  ti[2]=tt
  dis_f=0
  
 else
  dis_f=dis_f*0.8
  dis=flr(dis_f)
  hitf[1]=hitf[1]*0.5
 end
 for i=1,8 do
  hit[i]=flr(hitf[i])
 end
 
 hit[2]=(note[2])
 
 if hit[2]>2 then
  hitf[3]=2 --hit[2]
 else
  hitf[3]=hitf[3]*0.2
 end
 if note[2]==36 and ti[1]+dt*2>tt then 
  rnd_efu=rnd_efu+1 
  ti[1]=tt
 end
 if rnd_efu>8 then rnd_efu=0 end
 
 
 t=t+0.0005
 if rec==1 then
  update_music_stats()
 end
 yield()
end

