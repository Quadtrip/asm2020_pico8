function rand_poly(x1,y1,x2,y2)
 local i
 
 for i=1,#px do
  del(px,px[i])
  del(py,py[i])
 end
 for i=1,polys do
  add(px,mid(x1,x1+rnd(x2-x1),x2))
  add(py,mid(y1,y1+rnd(y2-y1),y2))
 end
end

function update_poly(hit)
 for i=1,#px do
  px[i]=px[i]-64
  px[i]=px[i]*(0.7+0.35*(hit[3]))
  px[i]=px[i]+64
  py[i]=py[i]-64
  py[i]=py[i]*(0.7+0.35*(hit[3]))
  py[i]=py[i]+64
 end
end

